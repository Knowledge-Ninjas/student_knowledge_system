<tr id="<%= dom_id course %>">

    <td>
      <%# <strong>Course name:</strong> %>
      <%= course.course_name %>
    </td>

    <td>
      <%# <strong>Section:</strong> %>
      <%= course.section %>
    </td>

    <td>
      <%# <strong>Teacher:</strong> %>
      <%= course.teacher %>
    </td>


    <td>
      <%# <strong>Semester:</strong> %>
      <%= course.semester %>
    </td>
    


    <td>
      <button class="btn btn-outline-dark"><%= link_to "View Profile", course %></button>
    </td>

</tr>